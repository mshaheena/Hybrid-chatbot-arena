import joblib
from sklearn.ensemble import RandomForestClassifier  

model = RandomForestClassifier()
model.fit([[0, 0], [1, 1]], [0, 1])  # Dummy training data

# ✅ Save model correctly
joblib.dump(model, "hybrid_model.pkl", protocol=4)  # Use protocol=4 to avoid compatibility issues

print("✅ Model successfully saved as hybrid_model.pkl")


